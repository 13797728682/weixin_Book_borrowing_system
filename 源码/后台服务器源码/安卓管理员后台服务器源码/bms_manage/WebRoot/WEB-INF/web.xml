<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>Login</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>cn.sdut.bms_manage.service.login.Login</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ReSetPWD</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>cn.sdut.bms_manage.service.forgetpwd.ReSetPWD</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Base64To</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>cn.sdut.bms_manage.service.test.Base64To</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LookRecord</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>cn.sdut.bms_manage.service.record.LookRecord</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/servlet/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ReSetPWD</servlet-name>
    <url-pattern>/servlet/ReSetPWD</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Base64To</servlet-name>
    <url-pattern>/servlet/Base64To</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LookRecord</servlet-name>
    <url-pattern>/servlet/LookRecord</url-pattern>
  </servlet-mapping>

</web-app>