<view id="container">
    <view class="img">
        <image src="{{src}}"></image>
    </view>
    <view class="book-name">
        <text>{{bookName}}</text>
    </view>
    <view class="author">
        <text>{{author}}</text>
    </view>
    <view class="book-id">
       <text>编号:</text> <text>{{isbn}}{{bookId}}</text>
    </view>

    <view class="addbtn">
