    <navigator url="{{url}}" open-type="{{openType}}" >
    
     <button type="primary" size="mini" style="background:{{background}}" >{{btnTxt}}</button>
    </navigator>


    <view class="rese-data">
    <text> 说明：{{explain}}</text>
    </view>
</view>

